<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape android:shape="rectangle" >
            <solid
                android:color="#0CA5B0"
                />
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle" >
            <corners
                android:radius="1dp"
                />
            <solid
                android:color="#4E3F30"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <stroke
                android:width="1dp"
                android:color="#4E3F30"
                />
        </shape>
    </item>
</selector>